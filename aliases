# Traversal ##########################
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias cd..='cd ..'
alias l='ls -al'

# VIM
alias vim='nvim'

# Config editing
alias zshconfig="vim ~/.zshrc"
alias vimconfig="vim ~/.vimrc"

# projects
alias conn='npm run connect:watch'
alias style='npm run style:watch'
alias jstest='npm run test:watch'

# love
alias love="/Applications/love.app/Contents/MacOS/love"
